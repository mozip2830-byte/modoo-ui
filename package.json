{
  "name": "modoo-ui",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -C web build",
    "start": "pnpm -C web start:prod",
    "dev:customer": "pnpm --filter modoo-customer start",
    "dev:partner": "pnpm --filter modoo-partner start",
    "dev:partner:offline": "powershell -NoProfile -Command \"$env:EXPO_OFFLINE='1'; pnpm --filter modoo-partner exec expo start -c --port 8082\"",
    "typecheck": "pnpm -r --if-present run typecheck"
  },
  "dependencies": {
    "@expo/metro-runtime": "^6.1.2",
    "firebase-admin": "^12.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "overrides": {
    "@firebase/app": "0.14.6",
    "@firebase/auth": "1.12.0",
    "@firebase/firestore": "4.9.3",
    "@firebase/functions": "0.13.1",
    "@firebase/storage": "0.14.0"
  }
}
